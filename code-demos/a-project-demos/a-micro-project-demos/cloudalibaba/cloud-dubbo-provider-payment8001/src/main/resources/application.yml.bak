server:
  port: 8001


spring:
  main:
    allow-circular-references: true
    allow-bean-definition-overriding: true
  application:
    # 应用名
    name: payment-provider-dubbo
  cloud:
      nacos:
        discovery:
          # Nacos服务地址
          server-addr: 192.168.66.100:8848

dubbo:
  # dubbo 服务实现类扫描包
   scan:
     base-packages: org.example.payment.zhifubao.service
   registry:
     #注册地址
     address: nacos://192.168.66.100:8848
     timeout: 10000
   protocol:
     # Dubbo服务暴露的协议的配置 dubbo
     name: dubbo
     # 端口从20880开始 自增
     port: -1










logging:
  pattern:
    #日志打印格式
    console: logging.pattern.console=%d{MM/dd HH:mm:ss.SSS} %clr(%-5level) ---  [%-15thread] %cyan(%-50logger{50}):%msg%n